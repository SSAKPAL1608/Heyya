            </class>
            <!-- Second test class with parameters -->
            <class name="TestNg.TestParameters2">
                <parameter name="browser" value="Firefox"/>
               
            </class>
        </classes>
    </test> <!-- Test1 -->

    <!-- Second Test -->
    <test name="Test2">
        <classes>
            <!-- First test class with parameters -->
